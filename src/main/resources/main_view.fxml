<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="800.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.views.MainView">
    <children>
        <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0"
                 AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <tabs>
                <Tab text="Recent transactions">
                    <content>
                        <fx:include source="home_view.fxml" fx:id="home_tab"/>
                    </content>
                </Tab>
                <Tab text="Preferences">
                    <content>
                        <fx:include source="preferences_view.fxml" fx:id="preferences_tab" />
                    </content>
                </Tab>
                <Tab text="Statistics">
                    <content>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
